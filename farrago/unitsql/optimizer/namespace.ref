. . . . . . . . >     exception_name,
. . . . . . . . >     p."name" as param_name
. . . . . . . . > from
. . . . . . . . >     (select n."name" as namespace_name,e.
. . . . . . . . >         "mofId" as e_id,e."name" as exception_name
. . . . . . . . >     from
. . . . . . . . >         mof_repository.model."Namespace" n
. . . . . . . . >     inner join
. . . . . . . . >         mof_repository.model."Exception" e
. . . . . . . . >     on n."mofId" = e."container") ne
