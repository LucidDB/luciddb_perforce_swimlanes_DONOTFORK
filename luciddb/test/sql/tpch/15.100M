0: jdbc:luciddb:> CREATE VIEW REVENUE0 (SUPPLIER_NO, TOTAL_REVENUE) AS
. . . . . . . . >     SELECT L_SUPPKEY, SUM(L_EXTENDEDPRICE * (1 - L_DISCOUNT))
. . . . . . . . >     FROM LINEITEM
. . . . . . . . >     WHERE
. . . . . . . . >         L_SHIPDATE >= DATE'1996-01-01' AND
. . . . . . . . >         L_SHIPDATE < DATE'1996-04-01'
. . . . . . . . >     GROUP BY L_SUPPKEY;
0: jdbc:luciddb:> 
0: jdbc:luciddb:> 
0: jdbc:luciddb:> SELECT S_SUPPKEY, S_NAME, S_ADDRESS,  S_PHONE, TOTAL_REVENUE
. . . . . . . . > FROM SUPPLIER, REVENUE0,
. . . . . . . . >      (SELECT MAX(TOTAL_REVENUE) AS MAXREV FROM REVENUE0) AS TEMP
. . . . . . . . > WHERE S_SUPPKEY = SUPPLIER_NO AND
. . . . . . . . >       TOTAL_REVENUE = TEMP.MAXREV
. . . . . . . . > ORDER BY S_SUPPKEY;
