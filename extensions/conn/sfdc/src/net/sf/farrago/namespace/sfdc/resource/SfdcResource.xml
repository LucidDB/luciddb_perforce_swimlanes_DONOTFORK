<?xml version="1.0" ?>
<?xml-stylesheet type="text/xsl"
 href="http://eigenbase.sourceforge.net/resgen/Resource.xsl" ?>
<resourceBundle
  static="false"
  locale="en_US"
  exceptionClassName="org.eigenbase.util.EigenbaseException">

  <!-- ====================================================================== -->
  <!-- Misc resources 80,000 - 85,000 -->
  <!-- ====================================================================== -->

  <exception id="80000" name="SfdcBinding_ServiceException">
    <text>Exception during attempted soap binding: {0}</text>
  </exception>

  <exception id="80010" name="SfdcLoginFault">
    <text>Exception during login to Salesforce: {0}</text>
  </exception>

  <exception id="80020" name="QueryException">
    <text>Exception trying to query object {0}: {1}</text>
  </exception>

  <exception id="80030" name="BindingCallException">
    <text>Exception querying binding: {0}</text>
  </exception>

  <exception id="80040" name="ReplicationException">
    <text>Cannot replicate data for object {0}</text>
  </exception>

  <exception id="80050" name="InvalidObjectException">
    <text>Invalid SFDC Object {0}</text>
  </exception>

  <exception id="80060" name="InvalidTimeException">
    <text>Invalid start and end times provided: {0}</text>
  </exception>

  <exception id="80070" name="InvalidRangeException">
    <text>Must provide a starting and ending TIMESTAMP range</text>
  </exception>

  <exception id="80080" name="InvalidStartTimeException">
    <text>Start date {0} must be within the last thirty days</text>
  </exception>

  <exception id="80090" name="InvalidEndTimeException">
    <text>End date {0} must come after Start date {1}</text>
  </exception>

  <exception id="80100" name="VarcharExceededPrecisionException">
    <text>String "{0} ..." truncated to precision {1}</text>
  </exception>

  <!-- ====================================================================== -->
  <!-- Server generated messages shown to end user -->
  <!-- ====================================================================== -->

  <message name="AllObjectQueryExceptionMsg">
    <text>Exception trying to retrieve all objects: {0}</text>
  </message>

  <message name="ObjectQueryExceptionMsg">
    <text>Exception trying to query object {0}: {1}</text>
  </message>

  <message name="IOExceptionMsg">
    <text>Exception with files: {0}</text>
  </message>

  <message name="MedSfdc_USER_NAME_Description">
    <text>User name to login to Salesforce</text>
  </message>

  <message name="MedSfdc_PASSWORD_Description">
    <text>Password to login to Salesforce</text>
  </message>

  <message name="MedSfdc_VARCHAR_FIELD_EXTRA_PRECISION_Description">
    <text>Extra precision to add to all Salesforce VARCHAR fields</text>
  </message>

  <message name="MedSfdc_ENDPOINT_URL_Description">
    <text>Web Service endpoint URL if not a default production environment</text>
  </message>

  <message name="LoggedInMsg">
    <text>Login successful</text>
  </message>

  <message name="RetrievedAllRecordsMsg">
    <text>Retrieved {0} records of "{1}". Done.</text>
  </message>

  <message name="RetrievingRecordsMsg">
    <text>Retrieved {0} records of "{1}". Not yet done.</text>
  </message>

  <message name="ObjectExtractErrorMsg">
    <text>Cannot extract object {0}; skipping.</text>
  </message>

</resourceBundle>

<!-- End SfdcResource.xml -->
